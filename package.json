    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "lucide-react": "^0.484.0",
    "next": "^15.3.0",
    "postcss": "^8.5.2",
    "react": "^18.3.1",
    "react-day-picker": "8.10.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.6",
    "tw-animate-css": "^1.2.4",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "3.2.4",
